        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "serverless-http": "^3.2.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@modelcontextprotocol/inspector": "^0.16.2",
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.21",
        "@types/node": "^22.7.4",
