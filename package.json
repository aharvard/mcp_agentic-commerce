    "scripts": {
        "dev": "tsx watch src/dev-server.ts",
        "dev:mcp": "tsx watch src/dev-server.ts",
        "prebuild": "tsx src/scripts/buildStyles.ts",
        "build": "npm run prebuild && tsc",
        "generate:data": "tsx src/scripts/generateMockRestaurants.ts",
        "generate:menus": "tsx src/scripts/generateMockMenus.ts",
        "start": "node dist/src/dev-server.js",
        "start:mcp": "node dist/src/dev-server.js",
        "dev:inspector": "CLIENT_PORT=6274 SERVER_PORT=6277 npx @modelcontextprotocol/inspector --config mcp.json --server agentic-commerce-dev",
        "inspect:dev": "CLIENT_PORT=6274 SERVER_PORT=6277 npx @modelcontextprotocol/inspector --config mcp.json --server agentic-commerce-dev",
        "inspect:prod": "CLIENT_PORT=6274 SERVER_PORT=6277 npx @modelcontextprotocol/inspector --config mcp.json --server agentic-commerce"
    },
    "dependencies": {
        "@mcp-ui/server": "^5.2.0",
        "@modelcontextprotocol/sdk": "^1.17.2",
        "@netlify/functions": "^2.5.1",
